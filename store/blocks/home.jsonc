{
  "store.home": {
    "blocks": ["rich-text",
      "list-context.image-list#home","tab-layout#home",
      "search-result-layout.customQuery#sale"
    ]
  }, 
  "rich-text": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "*Desafio Hiring Coders Vtex - UPMedal*",
      "textColor": "c-on-emphasis",
      "font": "t-heading-5"      
    }
  },
  //início slider
 "list-context.image-list#home": {
    "children": ["slider-layout#home"],
    "props": {
      "height": 300,
